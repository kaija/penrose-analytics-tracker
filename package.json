{
  "name": "analytics-tracker",
  "version": "1.0.0",
  "description": "A Woopra-like JavaScript analytics tracking system",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "USAGE.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "analytics",
    "tracking",
    "events",
    "woopra",
    "visitor-tracking",
    "event-tracking",
    "user-identification",
    "pageview",
    "click-tracking",
    "download-tracking"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/analytics-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/analytics-tracker/issues"
  },
  "homepage": "https://github.com/yourusername/analytics-tracker#readme",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "fast-check": "^3.15.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  }
}
